// While Loop Examples

tool countdown() {
    let count = 5
    while count > 0 {
        count = count - 1
    }
    return "done"
}

tool findFirst() {
    let items = [1, 3, 5, 7, 9]
    let i = 0
    while i < 5 {
        if items[i] > 5 {
            break
        }
        i = i + 1
    }
    return "found"
}
